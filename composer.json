{
    "name": "chiiya/laravel-tmdb",
    "description": "Laravel SDK for the TMDB API",
    "keywords": [
        "chiiya",
        "laravel",
        "tmdb",
        "movies",
        "tv"
    ],
    "homepage": "https://github.com/chiiya/laravel-tmdb",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Witte",
            "email": "github@chiiya.moe",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "chiiya/tmdb-php": "^1.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "chiiya/laravel-code-style": "^3.2",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.6",
        "phpunit/phpunit": "^12.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Chiiya\\LaravelTmdb\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chiiya\\LaravelTmdb\\LaravelTmdbServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
